{
  "main": "ncc.js",
  "targets": ["node20-linux-x64"],
  "compression": "GZip",
  "assets": [
    "node_modules/web-tree-sitter/tree-sitter.wasm",
    "node_modules/@cs/tree-sitter/tree-sitter-wasms/**/*",
    "node_modules/@cs/context-utils/dist/lib/node_modules/web-tree-sitter/tree-sitter.wasm",
    "node_modules/@cs/context-utils/dist/lib/node_modules/@cs/tree-sitter/tree-sitter-wasms/**/*",
    "node_modules/@cs/tree-sitter/node_modules/@cs/context-utils/dist/lib/node_modules/web-tree-sitter/tree-sitter.wasm",
    "node_modules/@cs/tree-sitter/node_modules/@cs/context-utils/dist/lib/node_modules/@cs/tree-sitter/tree-sitter-wasms/**/*",
    "node_modules/tiktoken/**/*.wasm",
    "src/transformers.js/dist/*.wasm",
    "node_modules/@lancedb/lancedb-linux-x64-gnu/lancedb.linux-x64-gnu.node",
    "node_modules/onnxruntime-node/bin/napi-v6/linux/x64/onnxruntime_binding.node",
    "node_modules/onnxruntime-node/bin/napi-v6/linux/x64/libonnxruntime.so.1",
    "node_modules/onnxruntime-node/bin/napi-v6/linux/x64/libonnxruntime_providers_shared.so",
    "node_modules/sqlite3/build/Release/*.node"
  ],
  "outputPath": "bin",
  "options": ["--no-warnings", "--no-bytecode"]
}
